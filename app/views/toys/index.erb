<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h2>Toys Inventory</h2>
		</div>
		<div class="col-md-6">
			<div class="row">
				<a href="/add.erb">
					<h5>Add New</h5>
				</a>
			</div>
			<div class="row">
				<h5>Assign to Box</h5>
			</div>
		</div>
	</div>
	<div class="row">
		<ol>
			<% @toys.each do |t| %>
	    	<li><a href="/toys/<%= t.id %>" ><%= t.name %></a></li>
	  	<% end %>
		</ol>
	</div>
</div>